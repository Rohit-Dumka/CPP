#include <iostream>
using namespace std;
int main() {
  int arr[]={1,2,3,4,5,6,7};
  int n=sizeof(arr)/sizeof(int);
  int s=0;
  int e=n-1;
  
  while(s<e)
  {
      if(arr[s]%2==0 && arr[e]%2==1)
      {
          swap(arr[s],arr[e]);
          s++;
          e--;
      }
      if(arr[e]%2==0 && arr[s]%2==0)
      {
          e--;
      }
      else
      {
          s++;
      }
  }
  
  for(int i=0;i<n;i++)
  {
      cout<<arr[i]<<" ";
  }
}
